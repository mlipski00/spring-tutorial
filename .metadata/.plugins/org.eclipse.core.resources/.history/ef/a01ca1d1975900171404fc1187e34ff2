package a_Zadania.a_Dzien_1.d_Konstruktor;

public class Burger {
	
	private String size;
	private int price; 

	public Burger() {
	 //konstruktor	bezargumentowy
		System.out.println("stworzenie nowego obektu klasy Burger");
	}
	public Burger(String size, int price) {
		this.size = size;
		this.price = price;
	}
}
