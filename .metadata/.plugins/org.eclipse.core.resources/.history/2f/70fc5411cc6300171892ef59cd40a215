package a_Zadania.a_Dzien_2.a_Dziedziczenie;

import a_Zadania.a_Dzien_2.a_Dziedziczenie.Shape;

public class Main2 {
	
	public static void main (String[] args) {
		
		Shape shape1 = new Shape(1, 2, "czerwony");
		Shape shape2 = new Shape(6, 4, "zielony");
		
		Circle circle1 = new Circle(1, 3, "czarny", 5);
		
		System.out.println(circle1.getArea(5));
		
		System.out.println(shape1.getDescription());
		
		System.out.println(circle1.getDescription());
		
		System.out.println(shape1.getDistance(shape2));
		
		Employee e1 = new Employee(1, "imie", "nazwisko", 10000);
		e1.raiseSalary(0);
		System.out.println(e1.salary);
	}

}
