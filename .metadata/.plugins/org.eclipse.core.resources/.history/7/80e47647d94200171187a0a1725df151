package a_Zadania.a_Dzien_1.b_Metody;
import java.util.Scanner;

public class Main1 {

    public static void main(String[] args) {
        
    	Scanner x = new Scanner(System.in);
    	
    	int y = x.nextInt();
    	System.out.print(square(y));
    }
    	public int square(int num) {
    	return num*num; }
    	
    	
    }
