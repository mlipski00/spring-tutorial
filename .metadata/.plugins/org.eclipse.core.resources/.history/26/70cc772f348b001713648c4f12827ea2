package a_Zadania.a_Dzien_2.b_Zaawansowana_obiektowosc;

public class Main1 {

	public static void main (String[] args) {
		
		printTriangle(4);
	}

	 static void printTriangle(int n) {
		 String x = "*";
		 String y = "*";
			for (int i = 1; i<=n; i++) {
				System.out.println(x);
				x +="*";
			}
	 
	}
}
