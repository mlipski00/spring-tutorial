package exam;

public class Main1 {

	public static void main(String[] args){
	System.out.println(rectangle(2, 4));
	System.out.println(rectangle(2, 0));
	System.out.println(rectangle('b', 4));
	}



	static int rectangle(int a, int b) {
	try {
		return a*b;
		}	
		catch (ArithmeticException ex) {
			System.out.println("Arithmetic Exception: You can't divide by 0");
		}
	return -1;
	}
}