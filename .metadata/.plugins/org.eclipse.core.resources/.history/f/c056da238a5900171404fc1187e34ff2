package a_Zadania.a_Dzien_1.b_Atrybuty;

public class Person {

public String name;
public String surname;
public int age;
public String gender;

	public String setAttr(String name, String surname, int age, String gender) {
		this.name = name;
		this.surname = surname;
		this.age = age;
		this.gender = gender;
	
	public String toString() {
	return this.name + this.surname + this.age + this.gender;
	}
}
}
