package a_Zadania.a_Dzien_2.b_Zaawansowana_obiektowosc;

public class Main2 {
	
	public static void main (String[] args) {
		
		Shape shape1 = new Shape(1, 2, "czerwony");
		Shape shape2 = new Shape(3, 4, "zielony");
		
		System.out.println(shape1.getDescription());
		
		System.out.println(shape1.getDistance(shape2));
	}

}
