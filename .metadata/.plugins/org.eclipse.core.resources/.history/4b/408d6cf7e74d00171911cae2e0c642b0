package bank;

public class Account {

	private String number;
	private double balance;
	
	public Account(String newNumber) {
		number = newNumber;
		balance = 0.0;
	}
	
	public double getBalance() {
		return balance;
	}
	public void setBalance(double balance) {
		this.balance = balance;
	}
	public String getNumber() {
		return number;
	}
}
