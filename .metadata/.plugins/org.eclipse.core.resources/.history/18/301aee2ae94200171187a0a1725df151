package a_Zadania.a_Dzien_1.b_Metody;


public class Main9 {

    public static void main(String[] args) {
        
    	System.out.print(factorial(-1));	
    }
    
    static int factorial(int n)
    {
    	int silnia = 1;
    	for (int m=1; m <= n; m++)
    		silnia *=m;
    	return silnia;
    		
    }
}
