package a_Zadania.a_Dzien_1.b_Metody;


public class Main9 {

    public static void main(String[] args) {
        
    	System.out.print(factorial(-2));	
    }
    
    static int factorial(int n)
    {
    	int silnia = 1;
    	if (n>=0) {
    	for (int m=1; m <= n; m++) {
    		silnia *=m;
    	}
    	return silnia; 
    	}
    	else {
    	throw new NumberFormatException("n nie jest liczbą naturalną");
    	}
    }
}
