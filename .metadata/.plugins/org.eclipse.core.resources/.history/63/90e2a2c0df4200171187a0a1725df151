package a_Zadania.a_Dzien_1.b_Metody;


public class Main6 {

    private static boolean x;
	public static void main(String[] args) {
        
    	System.out.print(checkMaturity(1));
    }
    static boolean checkMaturity(int age)
    {
    if (age >= 18)
     x = true;
    else
    x = false;
    return x;
    }
}
