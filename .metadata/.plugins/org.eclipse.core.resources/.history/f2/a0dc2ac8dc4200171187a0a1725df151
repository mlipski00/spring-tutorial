package a_Zadania.a_Dzien_1.b_Metody;


public class Main3 {

    public static void main(String[] args) {
        double x = 3.12;
    	System.out.println(convertToUsd(x));
    	System.out.println(convertToUsd(404));
    }
    
    static double convertToUsd(double zlotys)
    {
    	double wynik = zlotys * 4.04;
    	return wynik;
    }
}
